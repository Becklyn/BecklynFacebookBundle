{
    "name":         "oagm/facebook-bundle",
    "description":  "This bundle provides basic components for the usage with facebook",
    "keywords":     ["facebook"],
    "type":         "symfony-bundle",
    "homepage":     "https://bitbucket.org/becklyn/oagm-facebook-bundle",
    "authors":      [
        {
            "name":     "On Any Given Monday GmbH",
            "homepage": "http://onanygivenmonday.com/"
        }
    ],
    "require":      {
        "symfony/framework-bundle": "~2.0",
        "facebook/php-sdk": "~3.2"
    },
    "autoload":     {
        "psr-0": { "OAGM\\FacebookBundle": "" }
    },
    "target-dir": "OAGM/FacebookBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}